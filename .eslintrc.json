{
  "extends": "next/core-web-vitals",
  "rules": {
    "no-unused-vars": "off",
    "indent": [
      "error",
      2
    ],
    "no-duplicate-imports": [
      "error",
      {
        "includeExports": true
      }
    ],
    "linebreak-style": [
      "error",
      "unix"
    ],
    "quotes": [
      "error",
      "double"
    ],
    "semi": [
      "error",
      "never"
    ]
  }
}